{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/no-unresolved": "off",
      "class-methods-use-this":"off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          
         "ts": "never"
          
        }
        
      ],
      "camelcase": "off"
      
    },
    
    "settings": {
      "import/resolver": {
          "typescript": {}
      }
  }

    

}
